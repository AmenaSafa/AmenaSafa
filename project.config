[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Config files for my GitHub profile.
[access "refs/*"]
	owner = group user/AmenaSafa
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/AmenaSafa
	label-Verified = -1..+1 group user/AmenaSafa
	push = group user/AmenaSafa
	read = group user/AmenaSafa
	rebase = group user/AmenaSafa
	removeReviewer = group user/AmenaSafa
	submit = group user/AmenaSafa
[access "refs/heads/*"]
	create = group user/AmenaSafa
	push = +force group user/AmenaSafa
	pushMerge = group user/AmenaSafa
	read = group user/AmenaSafa
[access "refs/tags/*"]
	push = group user/AmenaSafa
	push = +force group user/AmenaSafa
